 {
 "manifest_version":2,
    "name":"TimeZone",
    "version":"2.0.1",
    "description":"This is a simple extention that displays your current time zone.",
    "icons":{
        "128":"clock.png"
    },
    "browser_action":{
        "default_popup":"popup.html",
        "default_icon":"clock.png",
        "default_title":"TimeZone"
    },
    "permissions":["activeTab"],
    "background":{
        "script":["main.js"]
    }
}